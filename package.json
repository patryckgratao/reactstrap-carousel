{
  "name": "reactstrap-carousel",
  "version": "0.0.18",
  "description": "Create custom carousels using bootstrap core.",
  "type": "module",
  "source": "src/index.js",
  "exports": {
    "import": "./dist/reactstrap.modern.js",
    "require": "./dist/reactstrap.cjs",
    "default": "./dist/reactstrap.modern.js"
  },
  "main": "./dist/reactstrap.cjs",
  "module": "./dist/reactstrap.module.js",
  "unpkg": "./dist/reactstrap.umd.js",
  "types": "types/index.d.ts",
  "esnext": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired test --env=jsdom",
    "cover": "npm test -- --coverage",
    "start": "start-storybook -s ./static -p 8080",
    "build:docs": "build-storybook -s ./static -o ./build",
    "build": "microbundle --generateTypes false --jsx React.createElement --compress false",
    "version": "node_modules/.bin/conventional-changelog -i CHANGELOG.md -o CHANGELOG.md -p angular && git add -A CHANGELOG.md",
    "prepublishOnly": "npm run build",
    "create-release": "npm run cover && sh ./scripts/release",
    "lint": "eslint src",
    "dtslint": "dtslint types --expectOnly"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "keywords": [
    "reactstrap",
    "bootstrap",
    "carousel",
    "slider",
    "vtex",
    "vtex io"
  ],
  "homepage": "https://github.com/patryckgratao",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@popperjs/core": "^2.6.0",
    "classnames": "^2.2.3",
    "interweave": "^12.9.0",
    "prop-types": "^15.5.8",
    "react-popper": "^2.2.4",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-storysource": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/react": "^16.9.51",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.1.0",
    "conventional-changelog-cli": "^2.0.21",
    "conventional-recommended-bump": "^4.1.1",
    "cross-env": "^4.0.0",
    "customize-cra": "^0.9.1",
    "dtslint": "^4.0.4",
    "ejs": "^2.5.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^5.0.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.0.0",
    "holderjs": "^2.9.3",
    "json-loader": "^0.5.7",
    "microbundle": "^0.13.3",
    "react": "^17.0.2",
    "react-app-rewired": "^1.6.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.0.3",
    "react-prism": "^4.3.2",
    "react-router": "^3.2.1",
    "react-scripts": "2.1.1",
    "react-test-renderer": "^16.3.2",
    "typescript": "^4.0.3",
    "webpack": "^4.43.0"
  }
}
